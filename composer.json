{
  "name": "thomasderooij/laravel-cqrs",
  "type": "project",
  "description": "A laravel CQRS class setup.",
  "keywords": [
    "cqrs",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9",
    "laravel/laravel": "*",
    "thomasderooij/cqrs": "^1"
  },
  "autoload": {
    "psr-4": {
      "Thomasderooij\\LaravelCqrs\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Thomasderooij\\LaravelCqrs\\Providers\\CqrsServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
